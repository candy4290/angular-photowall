{
    "name": "fis",
    "description": "this is fis",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "commitmsg": "validate-commit-msg",
        "precommit": "npm run lint",
        "commit": "git-cz",
        "ng": "ng",
        "ngc": "ngc",
        "rimraf": "rimraf",
        "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
        "build:ts": "ngc -p tsconfig-aot.json",
        "build": "ng build --prod --build-optimizer && http-server dist/ -p 8091 -o",
        "build:ks-main": "ng build --prod --build-optimizer --base-href /ks-main/resources/dist/",
        "build:doc": "npm run rimraf doc && typedoc --out ./doc/ ./src/app/pages/ --module commonjs --ignoreCompilerErrors && http-server doc -p 8092 -o",
        "clean:dist": "npm run rimraf node_modules dist aot",
        "reinstall": "npm run rimraf node_modules && npm install",
        "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "release:addChangeLog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "i18n": "ng-xi18n",
        "start": "ng serve --open",
        "start:hmr": "ng serve --hmr --verbose --open",
        "start:aot": "ng serve --aot --open",
        "start:proxy": "ng serve --proxy-config proxy.conf.json --open",
        "test": "ng test",
        "lint": "ng lint --type-check",
        "tslint": "tslint \"src/**/*.ts\"",
        "lint:styles": "stylelint ./src/**/*.scss",
        "e2e": "ng e2e",
        "eject": "ng eject"
    },
    "keywords": [
        "Angular 5+",
        "typescript"
    ],
    "engines": {
        "node": ">= 7.4.0",
        "npm": ">= 4.0.5",
        "@angular/cli": ">= 1.6.0"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^5.2.7",
        "@angular/core": "^5.2.7",
        "@angular/forms": "^5.2.7",
        "@angular/http": "^5.2.7",
        "@angular/platform-browser": "^5.2.7",
        "@angular/platform-browser-dynamic": "^5.2.7",
        "@angular/router": "^5.2.7",
        "@ngx-translate/core": "^9.1.1",
        "@ngx-translate/http-loader": "2.0.1",
        "angular-xyy": "^0.3.4",
        "angular2-notifications": "^0.9.7",
        "classlist.js": "^1.1.20150312",
        "core-js": "^2.5.3",
        "font-awesome": "^4.7.0",
        "intl": "^1.2.5",
        "ng-zorro-antd": "^0.6.15",
        "ngx-echarts": "^2.0.2",
        "primeng": "^5.2.0",
        "rxjs": "^5.5.6",
        "ts-helpers": "^1.1.2",
        "web-animations-js": "^2.3.1",
        "zone.js": "^0.8.20"
    },
    "devDependencies": {
        "@angular/animations": "^5.2.7",
        "@angular/cdk": "^5.2.3",
        "@angular/cli": "^1.7.2",
        "@angular/compiler": "^5.2.7",
        "@angular/compiler-cli": "^5.2.7",
        "@angular/language-service": "^5.2.7",
        "@angular/material": "^5.2.3",
        "@angular/platform-server": "^5.2.7",
        "@types/base64-js": "^1.2.5",
        "@types/hammerjs": "^2.0.35",
        "@types/jasmine": "2.8.6",
        "@types/jquery": "^3.3.0",
        "@types/node": "^9.4.6",
        "angular-ide": "^0.9.39",
        "angular-ide-loader": "^0.9.13",
        "angular2-jwt": "^0.2.3",
        "base64-js": "^1.2.3",
        "bootstrap": "^4.0.0",
        "codelyzer": "~4.2.1",
        "commitizen": "^2.9.6",
        "conventional-changelog": "^1.1.18",
        "conventional-changelog-cli": "^1.3.16",
        "cz-conventional-changelog": "^2.1.0",
        "echarts": "^4.0.4",
        "file-saver": "^1.3.3",
        "font-awesome-sass-loader": "^2.0.1",
        "gulp-angular2-embed-sass": "^0.5.2",
        "hammerjs": "^2.0.8",
        "http-server": "^0.11.1",
        "husky": "^0.14.3",
        "jasmine-core": "~3.1.0",
        "jasmine-spec-reporter": "~4.2.1",
        "jquery": "^3.3.1",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.4.1",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ng2-resource-rest": "^2.0.4",
        "ngx-bootstrap": "^2.0.2",
        "ngx-filesaver": "1.0.0",
        "popper.js": "^1.12.9",
        "protractor": "~5.3.0",
        "rimraf": "^2.6.2",
        "rollup": "^0.56.4",
        "rollup-plugin-commonjs": "^9.0.0",
        "rollup-plugin-node-resolve": "^3.0.3",
        "rollup-plugin-uglify": "^3.0.0",
        "source-map-explorer": "^1.5.0",
        "stylelint": "^9.1.1",
        "stylelint-config-standard": "^18.2.0",
        "ts-node": "~5.0.1",
        "tslint": "^5.9.1",
        "typedoc": "^0.11.0",
        "typescript": "~2.7.2",
        "util": "^0.10.3",
        "validate-commit-msg": "^2.14.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
